load "System";

Fibonacci(x) => a from {
	a = [0, 1];
	i in [3 : x] do {
		a push a[i - 1] + a[i - 2];
	};
};

len args == 0 then {
	Console.printLine(Fibonacci(10));
} else {
	Console.printLine(Fibonacci(num args[1]));
};

# Alternatively, the function can be done inline like so:
# [0 : 10] >> ( (x, a) -> { len a > 1 then a[len a] + a[len a - 1] else x } )
# This uses the >> operator, sending the array as the input arguments to a lambda one at a time.